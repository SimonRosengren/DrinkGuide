/client/src/components/drinkCard/drinkCard.js",["99","100"],"import React, { useState, useEffect } from \"react\";\nimport styles from \"./drinkCard.module.scss\";\nimport { useHistory } from \"react-router-dom\";\nfunction DrinkCard(props) {\n  const history = useHistory();\n  const handleClick = () => {\n      history.push(`/drink/${props.uuid}`)\n  }\n\n  return (\n    <div className={styles.wrapper}>\n      <div className={styles.imageWrapper}>\n        <img src={props.image} alt={props.name} onClick={handleClick} />\n      </div>\n      <div className={styles.textWrapper}>\n        <h2 className={styles.title}>{props.drinkName}</h2>\n        <p className={styles.description}>{props.drinkDescription}</p>\n      </div>\n    </div>\n  );\n}\n\nexport default DrinkCard;\n","/Users/simon.rosengren/pvtRepos/DrinkGuide/client/src/pages/Drink/Drink.js",["101","102","103"],"import React, { useState, useEffect } from \"react\";\nimport PropTypes from 'prop-types'\nimport styles from './drink.module.scss'\nimport { useParams } from 'react-router-dom';\nimport Recipe from '../../components/recipe';\n\nDrink.propTypes = {}\n\nexport default function Drink({}) {\n    const [drink, setDrink] = useState({});\n\n    let uuid = useParams();\n    useEffect(() => {  \n        (async () => {\n            const result = await (await fetch(`/api/recipe?uuid=${uuid.uuid}`)).json();\n            setDrink(result)\n        })();\n      }, [uuid]);\n\n    return (\n        <Recipe drink={drink}/>\n    )\n}","/Users/simon.rosengren/pvtRepos/DrinkGuide/client/src/components/recipe/index.js",[],"/Users/simon.rosengren/pvtRepos/DrinkGuide/client/src/components/recipe/recipe.js",["104"],"import React from 'react'\nimport PropTypes from 'prop-types'\nimport styles from \"./recipe.module.scss\";\n\nexport default function Recipe({drink}) {\n    return(<div className={styles.wrapper}>\n        <h2>{drink.name}</h2>\n        <h4>{drink.description}</h4>\n        <p>{drink.instructions}</p>\n        <ul>\n            {(drink.ingredients || []).map(ingredient => {\n                return <li>{ingredient.name}</li> // Ingredient links goes here\n            })}\n        </ul>\n    </div>);\n}","/Users/simon.rosengren/pvtRepos/DrinkGuide/client/src/components/Menu/index.js",[],"/Users/simon.rosengren/pvtRepos/DrinkGuide/client/src/components/Menu/Menu.js",["105","106","107","108","109","110"],"import React, { useState, useRef, useEffect } from 'react'\nimport PropTypes from 'prop-types'\nimport styles from './menu.module.scss'\nimport MenuItem from '../MenuItem'\n\nMenu.propTypes = {\n}\n\nexport default function Menu({ }) {\n    const [showMenu, setShowMenu] = useState(false);\n    const menuContainer = useRef(null);\n\n    useEffect(() => {\n        const handleEsc = (event) => {\n            if (event.keyCode === 27) {\n                setShowMenu(false);\n            }\n        };\n\n        const handleClickOutside = (event) => {\n            if (menuContainer && !menuContainer.current.contains(event.target)) {\n                setShowMenu(false);\n            }\n        };\n\n        window.addEventListener('keydown', handleEsc);\n        document.addEventListener('mousedown', handleClickOutside);\n\n        return () => {\n            window.removeEventListener('keydown', handleEsc);\n            document.removeEventListener('mousedown', handleClickOutside);\n        };\n    }, []);\n\n    return (\n        <nav role=\"navigation\" ref={menuContainer}>\n            <div id={styles.menuToggle}>\n                <input type=\"checkbox\" />\n                <span></span>\n                <span></span>\n                <span></span>\n                <ul id={styles.menu}>\n                    <a href=\"#\"><li>Login</li></a>\n                    <a href=\"/add-recipe\"><li>Add drink</li></a>\n                    <a href=\"#\"><li>About</li></a>\n                </ul>\n            </div>\n        </nav>)\n}","/Users/simon.rosengren/pvtRepos/DrinkGuide/client/sr